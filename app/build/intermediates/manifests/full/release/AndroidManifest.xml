<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.muhammadubaidullah.appic"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:icon="@drawable/appicon"
        android:label="Appic"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.muhammadubaidullah.appic.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.muhammadubaidullah.appic.MainActivity"
            android:label="Appic" />
        <activity
            android:name="com.example.muhammadubaidullah.appic.CreditsActivity"
            android:parentActivityName="com.example.muhammadubaidullah.appic.MainActivity" />
        <activity
            android:name="com.example.muhammadubaidullah.appic.ContainerActivityTwo"
            android:label="@string/title_activity_container_two"
            android:parentActivityName="com.example.muhammadubaidullah.appic.MainActivity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>